{
  "expo": {
    "name": "meu-perfil",
    "slug": "meu-perfil",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/foto-placeholder.jpg",
    "scheme": "meuperfil",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/foto-placeholder.jpg",
        "backgroundColor": "#0D1117"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/foto-placeholder.jpg"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/foto-placeholder.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0D1117"
        }
      ],
      "expo-router"
    ]
  }
}
